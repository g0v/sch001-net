  script.
    function runNums(target, targetNum, curNum = 0) {
      $(target).text(Math.floor(curNum));
      if (targetNum > curNum) {
        curNum++;
        if(targetNum - curNum > 20)
          curNum+=20;
        setTimeout(function() {
          runNums(target, targetNum, curNum)
        }, 40);
      }
    }

    function randNums(target, targetNum) {
      if($(target).hasClass("running")) {
        if(targetNum < 10) {
          $(target).text(Math.floor(9*Math.random()));
        }
        else if(targetNum < 100) {
          $(target).text(Math.floor(10 + 89*Math.random()));
        }
        else {
          $(target).text(Math.floor(100 + 899*Math.random()));
        }
        setTimeout(function() {
          randNums(target, targetNum)
        }, 10);
      } else {
        $(target).text(targetNum);
      }
    }

    // Add this new script at the end
    document.addEventListener('DOMContentLoaded', function() {
      // Function to open modal based on hash
      function openModalFromHash() {
        const hash = window.location.hash.substring(1);
        if (hash) {
          const modal = document.getElementById('teamModal-' + hash);
          if (modal) {
            const bsModal = new bootstrap.Modal(modal);
            bsModal.show();
          }
        }
      }

      // Open modal on page load if hash is present
      openModalFromHash();

      // Listen for hash changes
      window.addEventListener('hashchange', openModalFromHash);

      // Update hash when modal is shown
      document.querySelectorAll('.modal').forEach(modal => {
        modal.addEventListener('show.bs.modal', function () {
          const key = this.id.replace('teamModal-', '');
          history.pushState(null, null, '#' + key);
        });
      });

      // Remove hash when modal is hidden
      document.querySelectorAll('.modal').forEach(modal => {
        modal.addEventListener('hidden.bs.modal', function () {
          history.pushState('', document.title, window.location.pathname + window.location.search);
        });
      });

